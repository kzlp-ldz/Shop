<Window x:Class="Shop.BackgroundWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Shop"
        mc:Ignorable="d"
        Title="BackgroundWindow" Height="450" Width="800"
        FontFamily="Arial" 
        Background="#FFCFC2"
        Foreground="#AD7261">
    <Grid>
        <Rectangle  Fill="#FFCFC2"></Rectangle>
        <StackPanel  Orientation="Horizontal" VerticalAlignment="Top">
            <Image Source="/Images/logo.jpg" OpacityMask="#FFCFC2" Width="55" Margin="10" Height="55"></Image>
            
        </StackPanel>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Top" HorizontalAlignment="Right">
            <Button Width="55"  Content="Назад" Margin="10" Height="30" Name="BackBtn" Click="BackBtn_Click"></Button>
            <Button  Width="55" Content="Вперед" Margin="10"  Height="30"  Name="ForwardBtn" Click="ForwardBtn_Click"></Button>
        </StackPanel>

        <Frame x:Name="frame_auto_reg" NavigationUIVisibility="Hidden"/>
       
        
    </Grid>
</Window>
